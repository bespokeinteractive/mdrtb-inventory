<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.mdrtbinventory">
	<class name="org.openmrs.module.mdrtbinventory.InventoryDrugTransactionType" table="inventory_drug_transaction_type" batch-size="500">
		<id name="id" type="int" column="id" unsaved-value="0">
				<generator class="native" />
		</id>

		<property name="name" column="name" type="java.lang.String" />
		<property name="direction" column="direction" type="int" />
    </class>
</hibernate-mapping>
